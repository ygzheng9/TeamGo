<%= simple_form_for(@task) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :biz_owner %>
    <%= f.input :biz_plan_start_dt %>
    <%= f.input :biz_plan_end_dt %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
