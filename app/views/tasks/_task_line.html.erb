
<tr id="task_<%= task.id%>">
  <td><%= task.name %></td>
  <td><%= task.biz_owner.display_name unless (task.biz_owner.nil?) %></td>
  <td><%= task.biz_plan_start_dt %></td>
  <td><%= task.biz_plan_end_dt %></td>
  <td><%= link_to '修改', edit_task_path(task), remote: true %></td>
  <td><%= link_to '删除', task, method: :delete, remote:true, data: { confirm: 'Are you sure?' } %></td>
</tr>